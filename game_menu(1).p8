-- state management and init
state = "menu"  -- initial state
states = {}

-- main update function
function _update()
    states[state].update()  -- call the current state's update function
end

-- main draw function
function _draw()
   
    cls() 
    -- call the current state's draw function
    states[state].draw()
end


